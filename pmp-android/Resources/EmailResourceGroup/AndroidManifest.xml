<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="de.unistuttgart.ipvs.pmp.resourcegroups.email"
	android:versionCode="1" android:versionName="1.0">
	<uses-sdk android:minSdkVersion="7" />

	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:name="EmailResourceGroupApp">
		<activity android:label="@string/app_name"
			android:name="de.unistuttgart.ipvs.pmp.resource.ResourceGroupActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<service android:exported="true"
			android:name="de.unistuttgart.ipvs.pmp.service.resource.ResourceGroupService">
			<intent-filter>
				<action android:name="de.unistuttgart.ipvs.pmp.resourcegroups.email"></action>
			</intent-filter>
		</service>
	</application>
</manifest>