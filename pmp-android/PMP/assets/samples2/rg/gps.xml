<?xml version="1.0" encoding="utf-8"?>
<resourceGroupInformationSet>
	<resourceGroupInformation>
		<identifier>gov.gps</identifier>
		<defaultName lang="en">GPS</defaultName>
		<defaultDescription lang="en">The official GPS resource from the DoD that can track you - I mean - you can track your position.</defaultDescription>
	</resourceGroupInformation>
	<privacySettings>
		<privacySetting identifier="findloc">
			<!-- Valid value: "true", "false" -->
			<defaultName lang="en">Find Location</defaultName>
			<defaultDescription lang="en">May find your location. May maybe not.</defaultDescription>
		</privacySetting>
		<privacySetting identifier="modify">
			<!-- Valid value: "true", "false" -->
			<defaultName lang="en">Modify</defaultName>
			<defaultDescription lang="en">Gives the permission to modify your location (physically).</defaultDescription>
		</privacySetting>
		<privacySetting identifier="missiles">
			<!-- Valid value: "true", "false" -->
			<defaultName lang="en">Set Tomahawk Missile Target</defaultName>
			<defaultDescription lang="en">Errr... I forgot what that one was for...</defaultDescription>
		</privacySetting>		
	</privacySettings>
</resourceGroupInformationSet>