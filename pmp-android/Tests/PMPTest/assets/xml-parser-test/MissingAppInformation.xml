<?xml version="1.0" encoding="UTF-8"?>
<app>
	<serviceLevels>
		<defaultServiceLevel>
			<level>0</level>
			<defaultName lang="en">Null-Level</defaultName>
			<name lang="de">Null-Level</name>
			<name lang="sv">null nivå</name>
			<defaultDescription lang="en">I can't do anything for you!</defaultDescription>
			<description lang="de">Ich kann nichts für Sie tun!</description>
			<description lang="sv">Jag kan inte göra något för dig!</description>
		</defaultServiceLevel>
		<serviceLevel>
			<level>1</level>
			<defaultName lang="en">Level Complete</defaultName>
			<name lang="de">Vollständiges Level</name>
			<name lang="sv">Klara nivå</name>
			<defaultDescription lang="en">I can do anything you want</defaultDescription>
			<description lang="de">Ich kann alles, was Sie wünschen!</description>
			<description lang="sv">Jag kan göra vad du vill!</description>
			<requiredResourceGroup identifier="de.unistuttgart.ipvs.pmp.resourceGroups.database">
					<privacyLevel identifier="read">true</privacyLevel>
					<privacyLevel identifier="write">true</privacyLevel>
			</requiredResourceGroup>
			<requiredResourceGroup identifier="de.unistuttgart.ipvs.pmp.resourceGroups.filesystem">
					<privacyLevel identifier="read">true</privacyLevel>
					<privacyLevel identifier="write">true</privacyLevel>
					<privacyLevel identifier="maxFileSizeForWriting">
						<![CDATA[<value>20</value><unit>mb</unit>]]>
					</privacyLevel>
			</requiredResourceGroup>
		</serviceLevel>
	</serviceLevels>
</app>
	