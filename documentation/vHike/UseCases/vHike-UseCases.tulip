      <useCase>
        <id>35</id>
        <name>Registrierung</name>
        <useCaseId>01</useCaseId>
        <goal>&lt;p style="margin-top: 0"&gt; Der Benutzer m&amp;#246;chte einen Account f&amp;#252;r vHike erstellen. &lt;/p&gt;</goal>
        <description>&lt;p style="margin-top: 0"&gt; Der Benutzer m&amp;#246;chte einen Account f&amp;#252;r vHike erstellen. &lt;/p&gt;</description>
