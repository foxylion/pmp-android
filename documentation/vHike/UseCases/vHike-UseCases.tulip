        <author/>
        <lastUpdate>25-11-2011 13:57</lastUpdate>
        <actors>
          <actorId>37</actorId>
        </actors>
        <mainScenario>
          <name/>
          <preConditions>
            <preCondition>
              <description>vHike ist gestartet</description>
            </preCondition>
            <preCondition>
              <description>Benutzer hat noch keinen Account</description>
            </preCondition>
            <preCondition>
              <description>Niemand ist in vHike eingelogt</description>
            </preCondition>
          </preConditions>
          <steps>
            <step>
              <stepNumber>1</stepNumber>
              <actorId>37</actorId>
              <description>wählt die Registrierungsoption aus</description>
            </step>
            <step>
              <stepNumber>2</stepNumber>
              <actorId/>
              <description>zeigt das Fenster mit den Anmeldedaten an</description>
            </step>
            <step>
              <stepNumber>3</stepNumber>
              <actorId>37</actorId>
              <description>fühlt das Formular aus und bestehtigt es</description>
            </step>
            <step>
              <stepNumber>4</stepNumber>
              <actorId/>
              <description>erstelt den Login-Account für den Benutzer</description>
            </step>
            <step>
              <stepNumber>5</stepNumber>
              <actorId/>
              <description>zeigt wieder den Login-Screen an</description>
            </step>
