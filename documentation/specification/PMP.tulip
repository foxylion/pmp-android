          <useCase>
            <id>24</id>
            <name>Ressourcengruppe installieren</name>
            <useCaseId>300</useCaseId>
            <goal>&lt;p style="margin-top: 0"&gt; Der Benutzer m&amp;#246;chte eine Ressourcengruppe finden und installieren. &lt;/p&gt;</goal>
            <description/>
            <priority>Niedrig</priority>
            <level>Technische Details</level>
            <created>03-11-2011 16:21</created>
            <author>tk</author>
            <lastUpdate>08-11-2011 15:06</lastUpdate>
            <primaryActorId>6</primaryActorId>
            <actors>
              <actorId>6</actorId>
              <actorId>13</actorId>
            </actors>
            <mainScenario>
              <name/>
              <preConditions>
                <preCondition>
                  <description>das PMP-Menü ist geöffnet</description>
                </preCondition>
                <preCondition>
                  <description/>
                </preCondition>
              </preConditions>
              <steps>
                <step>
                  <stepNumber>1</stepNumber>
                  <actorId>6</actorId>
                  <description>wählt die Paketmanager-Ansicht aus.</description>
                </step>
                <step>
                  <stepNumber>2</stepNumber>
                  <actorId/>
                  <description>ruft vom Paketmanagerserver die aktuelle Titelseite ab.</description>
                </step>
                <step>
                  <stepNumber>3</stepNumber>
                  <actorId/>
                  <description>zeigt die Titelseiten-Ansicht und ein Suchfeld.</description>
                </step>
                <step>
                  <stepNumber>4</stepNumber>
                  <actorId>6</actorId>
                  <description>tippt in das Suchfeld einen Suchstring für die gewünschte Ressourcengruppe.</description>
                </step>
                <step>
                  <stepNumber>5</stepNumber>
                  <actorId/>
                  <description>fragt den Paketmanagerserver nach diesem Suchstring.</description>
                </step>
                <step>
                  <stepNumber>6</stepNumber>
                  <actorId>13</actorId>
                  <description>sucht nach passenden Ressourcengruppen und sendet deren Informationen zurück.</description>
                </step>
                <step>
                  <stepNumber>7</stepNumber>
                  <actorId/>
                  <description>zeigt dem Benutzer die Übersicht über die Ressourcengruppen.</description>
                </step>
                <step>
                  <stepNumber>8</stepNumber>
                  <actorId>6</actorId>
                  <description>wählt eine Ressourcengruppe aus.</description>
                </step>
                <step>
                  <stepNumber>9</stepNumber>
                  <actorId/>
                  <description>zeigt dem Benutzer die Details der ausgewählten Benutzergruppe.</description>
                </step>
                <step>
                  <stepNumber>10</stepNumber>
                  <actorId>6</actorId>
                  <description>wählt die Installieren-Funktion.</description>
                </step>
                <step>
                  <stepNumber>11</stepNumber>
                  <actorId/>
                  <description>lädt vom Paketmanagerserver die ausgewählte Ressourcengruppe herunter.</description>
                </step>
                <step>
                  <stepNumber>12</stepNumber>
                  <actorId/>
                  <description>überprüft, ob die Signatur der Ressourcengruppe korrekt ist.</description>
                </step>
                <step>
                  <stepNumber>13</stepNumber>
                  <actorId/>
                  <description>entpackt den Code der Ressourcengruppe und fügt sie in die Datenbank ein.</description>
                </step>
                <step>
                  <stepNumber>14</stepNumber>
                  <actorId/>
                  <description>startet die Ressourcengruppe.</description>
                </step>
                <step>
                  <stepNumber>15</stepNumber>
                  <actorId/>
                  <description>zeigt an, dass die Ressourcengruppe erfolgreich installiert wurde.</description>
                </step>
                <step>
                  <stepNumber>16</stepNumber>
                  <actorId/>
                  <description>zeigt die Titelseiten-Ansicht und ein Suchfeld.</description>
                </step>
              </steps>
              <postConditions>
                <postCondition>
                  <description>die gewünschte Ressourcengruppe ist installiert</description>
                </postCondition>
              </postConditions>
            </mainScenario>
            <extensions>
              <extensionScenario>
                <associatedStep>2</associatedStep>
                <name>2a</name>
                <preConditions>
                  <preCondition>
                    <description>Es konnte keine Verbindung zum Paketmanagerserver hergestellt werden.</description>
                  </preCondition>
                </preConditions>
                <steps>
                  <step>
                    <stepNumber>2a1</stepNumber>
                    <actorId/>
                    <description>zeigt statt der Titelseite eine Fehlermeldung und fordert den Benutzer dazu auf, eine Verbindung zu ermöglichen, wenn er neue Ressourcengruppe installieren will.</description>
                  </step>
                </steps>
                <postConditions>
                  <postCondition>
                    <description>es gab noch keine Änderung der installierten Ressourcengruppen</description>
                  </postCondition>
                </postConditions>
              </extensionScenario>
              <extensionScenario>
                <associatedStep>7</associatedStep>
