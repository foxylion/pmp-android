            <author/>
            <lastUpdate>04-11-2011 23:02</lastUpdate>
            <primaryActorId>6</primaryActorId>
            <actors>
              <actorId>6</actorId>
            </actors>
            <mainScenario>
              <name/>
              <preConditions>
                <preCondition>
                  <description>PMP ist gestartet</description>
                </preCondition>
                <preCondition>
                  <description>Es ist noch kein Untermenü geöffnet</description>
                </preCondition>
                <preCondition>
                  <description>Es ist mindestens eine Ressourcengruppe installiert</description>
                </preCondition>
              </preConditions>
              <steps>
                <step>
                  <stepNumber>1</stepNumber>
                  <actorId>6</actorId>
                  <description>wählt die Ressourcengruppenübersicht aus</description>
                </step>
                <step>
                  <stepNumber>2</stepNumber>
                  <actorId/>
                  <description>zeigt alle verfügbaren Ressourcengruppen an</description>
                </step>
                <step>
                  <stepNumber>3</stepNumber>
                  <actorId>6</actorId>
                  <description>wählt eine Ressourcengruppe aus</description>
                </step>
                <step>
                  <stepNumber>4</stepNumber>
                  <actorId/>
                  <description>zeigt die Details der gewählten Ressourcengruppe an</description>
                </step>
                <step>
                  <stepNumber>5</stepNumber>
                  <actorId>6</actorId>
                  <description>schließt die Detailansicht der Ressourcengruppe</description>
                </step>
                <step>
                  <stepNumber>6</stepNumber>
                  <actorId/>
                  <description>zeigt alle verfügbaren Ressourcengruppen wieder an</description>
                </step>
                <step>
                  <stepNumber>7</stepNumber>
                  <actorId>6</actorId>
                  <description>schließt die Ressourcengruppenübersicht</description>
                </step>
                <step>
                  <stepNumber>8</stepNumber>
                  <actorId/>
                  <description>kehrt zum Hauptmenü zurück</description>
                </step>
              </steps>
              <postConditions>
                <postCondition>
                  <description>Details zu einer Ressourcengruppe angezeigt</description>
                </postCondition>
              </postConditions>
            </mainScenario>
            <extensions/>
          </useCase>
        </children>
